{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "indent": [2, 2, { "SwitchCase": 1 }],
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "react/forbid-prop-types": [2, {
      "forbid": []
    }]
  }
}